{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [ "plugin:@wordpress/eslint-plugin/recommended" ],
	"rules": {
		"react/prop-types": "off",
		"jsdoc/check-tag-names": "off",
		"jsx-a11y/label-has-associated-control": "off",
		"import/no-unresolved": [
			"error",
			{ "ignore": [ "^jquery$", "preact", "@wordpress" ] }
		]
	}
}
